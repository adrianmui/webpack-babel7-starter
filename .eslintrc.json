{
	"extends": [
			"airbnb-base", 
			"prettier"
	],
	"env": {
			"es6": true,
			"node": true,
			"commonjs": true
	},
	"plugins": [
			"prettier"
	],
	"parser": "babel-eslint",
	"parserOptions": {
			"ecmaVersion": 2017,
			"sourceType": "module"
	},
	"settings": {
		"import/resolver": {
			"node": {
				"extensions": [
					".js",
					".jsx"
				]
			}
		}
	},
	"rules": {
			"prettier/prettier": "error"
	}
}